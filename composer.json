{
  "name": "phoneburner/avalara-for-communications-php-sdk",
  "description": "Avalara For Communications PHP API client",
  "keywords": [ "avalara", "afc", "communications", "api" ],
  "license": "MIT",
  "authors": [
    {
      "name": "John Congdon",
      "email": "jcongdon@phoneburner.com",
      "homepage": "http://phoneburner.com"
    }
  ],
  "require": {
    "php": ">=7.4",
    "guzzlehttp/guzzle": "~6.0 || ^7"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "^9",
    "phpspec/prophecy-phpunit": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "PhoneBurner\\AvalaraAFC\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PhoneBurner\\AvalaraAFC\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "minimum-stability": "stable"
}
